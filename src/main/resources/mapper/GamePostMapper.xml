<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ac.kmgames.model.mapper.GamePostMapper">

    <select id="getGameTitle" resultType="String">
        SELECT game_title
        FROM game_post
        WHERE game_post_id = #{id}
    </select>

</mapper>